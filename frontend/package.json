{
  "name": "vitely",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Vitely, starter vite project with goodies",
  "author": "Rubiin",
  "keywords": [
    "vue",
    "typescript",
    "atomic-design",
    "ddd",
    "unocss",
    "ddd-example",
    "boilerplate",
    "vue-boilerplate",
    "vite"
  ],
  "scripts": {
    "dev": "vite --open --host --mode dev",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint -c .eslintrc.js --ext .js,.vue,.ts, src",
    "lint:fix": "eslint -c .eslintrc.js --fix --ext .js,.vue,.ts, src",
    "format": "prettier . --write",
    "typecheck": "vue-tsc --noEmit",
    "mock-db": "json-server --watch ./db/db.json",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "preview-https": "serve dist - 8080",
    "report": "vite build --mode report",
    "stylelint": "stylelint --config stylelintrc.json 'src/**/*.{vue,scss}'",
    "stylelint:fix": "stylelint --config stylelintrc.json --fix 'src/**/*.{vue,scss}'"
  },
  "dependencies": {
    "@quasar/extras": "^1.12.4",
    "@vueuse/core": "^7.5.5",
    "@vueuse/head": "^0.7.5",
    "axios": "^0.25.0",
    "pinia": "^2.0.9",
    "pinia-plugin-persistedstate": "^1.1.0",
    "quasar": "^2.4.13",
    "vue": "^3.2.28",
    "vue-i18n": "^9.2.0-beta.26",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@iconify-json/bx": "^1.0.3",
    "@iconify-json/carbon": "^1.0.14",
    "@intlify/vite-plugin-vue-i18n": "^3.2.1",
    "@quasar/vite-plugin": "^1.0.4",
    "@rollup/plugin-strip": "^2.1.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/vue": "^6.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.12",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@unocss/preset-icons": "^0.22.6",
    "@unocss/preset-uno": "^0.22.6",
    "@vitejs/plugin-vue": "^2.1.0",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "cypress": "^9.3.1",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.3.0",
    "postcss": "^8.4.5",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "prettier": "^2.5.1",
    "rollup-plugin-visualizer": "^5.5.4",
    "rtlcss": "^3.5.0",
    "sass": "1.32.0",
    "serve": "^13.0.2",
    "stylelint": "^14.3.0",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-standard": "^24.0.0",
    "typescript": "^4.5.5",
    "unocss": "^0.22.6",
    "unplugin-auto-import": "^0.5.11",
    "unplugin-vue-components": "^0.17.14",
    "vite": "^2.7.13",
    "vite-plugin-compress": "^2.1.0",
    "vite-plugin-fonts": "^0.2.2",
    "vite-plugin-pwa": "^0.11.13",
    "vite-svg-loader": "^3.1.2",
    "vitest": "^0.2.3",
    "vue-tsc": "^0.31.1"
  },
  "engines": {
    "node": ">= 14",
    "npm": ">= 8"
  }
}
