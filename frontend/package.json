{
  "name": "vitely",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Vitely, starter vite project",
  "keywords": [
    "vue",
    "typescript",
    "atomic-design",
    "ddd",
    "spa",
    "atomic-design-example",
    "ddd-example",
    "boilerplate",
    "vue-boilerplate",
    "vite"
  ],
  "scripts": {
    "dev": "vite --open --host --mode dev",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint -c .eslintrc.js --ext .js,.vue,.ts, src",
    "lint:fix": "eslint -c .eslintrc.js --fix --ext .js,.vue,.ts, src",
    "format": "prettier . --write",
    "typecheck": "vue-tsc --noEmit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "preview-https": "serve dist - 8080",
    "report": "vite build --mode report"
  },
  "dependencies": {
    "@vueuse/core": "^7.5.4",
    "@vueuse/head": "^0.7.5",
    "axios": "^0.25.0",
    "naive-ui": "^2.24.1",
    "pinia": "^2.0.9",
    "pinia-plugin-persistedstate": "^1.0.3",
    "vfonts": "^0.0.3",
    "vue": "^3.2.28",
    "vue-i18n": "^9.2.0-beta.26",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@iconify-json/bx": "^1.0.3",
    "@intlify/vite-plugin-vue-i18n": "^3.2.1",
    "@storybook/addon-actions": "^6.5.0-alpha.22",
    "@storybook/addon-essentials": "^6.5.0-alpha.22",
    "@storybook/addon-links": "^6.5.0-alpha.22",
    "@storybook/vue3": "^6.5.0-alpha.22",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/vue": "^6.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@unocss/preset-icons": "^0.22.5",
    "@unocss/preset-uno": "^0.22.5",
    "@vitejs/plugin-vue": "^2.1.0",
    "cypress": "^9.3.1",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-storybook": "^0.5.6",
    "eslint-plugin-vue": "^8.3.0",
    "prettier": "^2.5.1",
    "rollup-plugin-visualizer": "^5.5.4",
    "sass": "^1.49.0",
    "serve": "^13.0.2",
    "storybook-builder-vite": "^0.1.13",
    "typescript": "^4.5.5",
    "unocss": "^0.22.5",
    "unplugin-auto-import": "^0.5.11",
    "unplugin-vue-components": "^0.17.14",
    "vite": "^2.7.13",
    "vite-imagetools": "^4.0.3",
    "vite-plugin-compress": "^2.1.0",
    "vite-plugin-purge-icons": "^0.7.0",
    "vite-plugin-pwa": "^0.11.13",
    "vite-plugin-remove-console": "^0.0.6",
    "vite-plugin-webfont-dl": "^1.0.7",
    "vite-svg-loader": "^3.1.2",
    "vitest": "^0.2.0",
    "vue-tsc": "^0.31.1"
  }
