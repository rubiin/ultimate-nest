  "scripts": {
    "build": "nest build",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "orm": "npx mikro-orm",
    "sample": "cd env; npx  sample-env --env .env.dev",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:hmr": "nest build --watch --webpack --webpackPath webpack-hmr.js",
    "start:prod": "node dist/main",
    "start:repl": "MIKRO_ORM_ALLOW_GLOBAL_CONTEXT=true nest start --watch --entryFile repl",
    "start:debug": "NEST_DEBUG=true nest start --debug --watch",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand --forceExit"
  },
