	"scripts": {
		"prebuild": "rimraf dist",
		"build": "nest build",
		"postinstall": "npm run build",
		"format": "prettier --write --cache \"src/**/*.ts\" \"test/**/*.ts\"",
		"lint": "eslint_d '{src,test}/**/*.ts' --cache",
		"lint:fix": "eslint_d '{src,test}/**/*.ts' --cache --fix",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:prod": "node dist/main",
		"start:hmr": "nest build --watch --webpack --webpackPath webpack-hmr.js",
		"start:debug": "nest start --debug --watch",
		"test": "jest --verbose",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json --runInBand --forceExit",
		"orm": "npx mikro-orm"
	},
