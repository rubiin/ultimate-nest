	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"testTimeout": 30000,
		"moduleNameMapper": {
			"^@entities": "<rootDir>/entities/index",
			"^@mocks": "<rootDir>/_mocks_/index",
			"^@common/(.*)$": "<rootDir>/common/$1",
			"^@modules/(.*)$": "<rootDir>/modules/$1",
			"^@lib/(.*)$": "<rootDir>/lib/$1"
		},
		"rootDir": "src",
		"testRegex": ".spec.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "@swc/jest"
		},
		"runtime": "@side/jest-runtime",
		"coverageDirectory": "../coverage",
		"testEnvironment": "node",
		"coverageReporters": [
			"text",
			"html"
		]
	},
	"mikro-orm": {
		"useTsNode": true,
		"configPaths": [
			"./src/common/database/mikro-orm-cli.config.ts",
			"./dist/common/database/mikro-orm-cli.config.js"
		]
	},
	"engines": {
		"node": ">=18",
		"pnpm": ">=7"
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"eslint_d '{src,test}/**/*.ts' --cache --fix",
			"prettier --cache --write"
		],
		"*.{json,md,yaml,yml}": [
			"prettier --cache --write"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
